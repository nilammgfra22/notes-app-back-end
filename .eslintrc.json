{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "camelcase": 0
    }
}
